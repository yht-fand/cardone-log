<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd"
       default-lazy-init="true">
    <!-- 操作日志 -->
   <bean name="top.cardone.log.action.InsertOperateLogByEventListenerAction" class="top.cardone.log.action.InsertOperateLogByEventListenerAction">
       <property name="skipClassNameList">
           <list>
               <value>*.Log*</value>
               <value>*.Error*</value>
               <value>*.Count*</value>
               <value>*.RolePermission*</value>
               <value>*.UserPermission*</value>
               <value>*.I18nInfo*</value>
               <value>*.Navigation*</value>
               <value>*.Dictionary*</value>
           </list>
       </property>
   </bean>
</beans>